graph {

}
node {
    shape: box;
    fill-color: blue;
    stroke-mode: plain;
    stroke-color: #333;
    size:14px,14px;
    text-mode: normal;
    text-style: bold;
    text-alignment: at-left;
    text-background-mode: rounded-box;
    text-background-color: #EB2;
    text-color: #222;
    text-size: 17px;
}

node:clicked {
    fill-color: red;
}
